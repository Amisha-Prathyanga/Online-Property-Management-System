<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>User Management Web Application</display-name>
  <error-page>
    <exception-type>java.lang.Exception</exception-type>
    <location>/Error.jsp</location>
  </error-page>
  <servlet>
    <description></description>
    <display-name>WantedServlet</display-name>
    <servlet-name>WantedServlet</servlet-name>
    <servlet-class>com.wanted.WantedServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>WantedServlet</servlet-name>
    <url-pattern>/WantedServlet</url-pattern>
  </servlet-mapping>
</web-app>